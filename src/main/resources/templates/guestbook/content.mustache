<h1 class="mt-4">
    GuestBook List Page
</h1>
<table class="table table-striped">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Title</th>
        <th scope="col">Writer</th>
        <th scope="col">RegDate</th>
    </tr>
    </thead>
    <tbody>
    {{#result.dtoList}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{title}}</td>
            <td>{{writer}}</td>
            <td>{{formattedRegDate}}</td>
        </tr>
    {{/result.dtoList}}
    </tbody>
</table>